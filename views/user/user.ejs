    <section class="product-tabs section-padding wow fadeIn animated">
      <div class="container">
      
        <div class="tab-content wow fadeIn animated" id="myTabContent">
          <div
            class="tab-pane fade show active"
            id="tab-one"
            role="tabpanel"
            aria-labelledby="tab-one"
          >
          <div class="row product-grid-4">
            <% data.forEach(function(data){ %>
              <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                <div class="product-cart-wrap mb-30">
                  <div class="product-img-action-wrap">
                    <div class="product-img product-img-zoom">
                      <a href="/fullImage/<%=data._id%>">
                        <img
                          class="default-img"
                          src="/images/<%=data.Image[0]%>" style="height: 200px;"
                          alt=""
                        />
                        <!-- <img
                          class="hover-img"
                          src="/images/<%=data.Image%>"
                          alt=""
                        /> -->
                      </a>
                    </div>
                    <div class="product-action-1">
                      <button
                        type="button"
                        aria-label="Add To Wishlist"
                        class="action-btn hover-up"
                        onclick="addToWishlist('<%=data._id%>')
                      Swal.fire({
                        position: 'center',
                        icon: 'success',
                        title: 'Product is successfully added to wishlist',
                        showConfirmButton: false,
                        timer: 1500
                      })"
                      >
                        <i class="fi-rs-heart"></i>
                      </button>
                    </div>
                    <div
                      class="product-badges product-badges-position product-badges-mrg"
                    >
                      <span class="hot">Hot</span>
                    </div>
                  </div>
                  <div class="product-content-wrap">
                    <div class="product-category">
                      <a href="/fullImage/<%=data._id%>"><%=data.Category%></a>
                    </div>
                    <h2>
                      <a href="/fullImage/<%=data._id%>"
                        ><%=data.ProductName%></a
                      >
                    </h2>
                    <!-- <div class="rating-result" title="90%">
                      <span>
                        <span>90%</span>
                      </span>
                    </div> -->
                    <div class="product-price">
                      <span>₹<%=data.OfferPrice%></span>
                      <span class="old-price">₹<%=data.Price%></span>
                    </div>
                    <% if(data.Quantity>0){ %>
                      <div class="product-action-1 show">
                        <% if(!data.carted){%>
                          <button aria-label="Add To Cart" class="action-btn hover-up" onclick="addToCart('<%= data._id %>'); Swal.fire({
                            position: 'center',
                            icon: 'success',
                            title: 'Item Added Successfully To Cart',
                            showConfirmButton: false,
                            timer: 2000
                          })">
                            <i class="fi-rs-shopping-bag-add"></i>
                          </button>
                
                       <% }else{%>
                        <a href="/cart">
                       <button aria-label="View Cart"><i class="fa-sharp fa-solid fa-eye" style="color: #000000;"></i></button></a>
                       <% }%>
                        
                    </div>
                     <%}else{ %>
                      <div class="product-action-1 show">
                        <p class="text-danger" style="font-size: 12px;">out-of-stock</p>
                    </div>
                     <% } %>
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
            <!--End product-grid-4-->
          
            <div class="container">
              <div class="row">
                <div class="col-lg-12 d-flex justify-content-center">
                  <nav aria-label="Page">
