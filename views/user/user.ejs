   <script>
  function addToCart(proId){
    $.ajax({
      url:'/addToCart/'+proId,
      method:'get',
       success:(response)=>{
        if(response.status){
          let count=$('#cartCount').html()
          count = parseInt(count)+1;
          $('#cartCount').html(count)
        }
       }
    })
  }

  function addToWishlist(proId){
    $.ajax({
      url:'/wishList/'+proId,
      method:'get',
      success:(response)=>{
        if(response.status){
          let count =$('#wishListCount').html()
          count = parseInt(count)+1
          $('#wishListCount').html(count)
        }

      }
    })
  }

</script>
